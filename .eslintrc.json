{
  "extends": [
    "zeal",
    "zeal/react"
  ],
  "root": true,
  "rules": {
    "comma-dangle": [1, "only-multiline"],
    "jsx-quotes": [1, "prefer-double"],
    "react/prefer-stateless-function": [0],
    "no-unused-vars": [1, {
      "args": "all", "argsIgnorePattern": "^_",
      "vars": "all", "varsIgnorePattern": "^_"
    }]
  },
  "settings": {
    "import/resolver": "webpack"
  }
}
